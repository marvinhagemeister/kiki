{
  "name": "kiki-bundler",
  "version": "1.2.0",
  "description": "Batteries included frontend asset bundler",
  "main": "dist/index.js",
  "bin": {
    "kiki": "bin/kiki"
  },
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:marvinhagemeister/kiki.git"
  },
  "bugs": {
    "url": "https://github.com/marvinhagemeister/kiki/issues"
  },
  "homepage": "https://github.com/marvinhagemeister/kiki#readme",
  "scripts": {
    "pretest": "npm run build",
    "test": "mocha 'test/**/*.spec.js'",
    "test-watch": "npm run test -s -- -w",
    "clean": "rimraf 'dist/**/*.js' 'dist/**/*.js'",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint 'src/**/*.ts'",
    "precommit": "npm run lint && npm run test"
  },
  "author": "Marvin Hagemeister <marvin@marvinhagemeister.de>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.4.1",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.0",
    "cross-spawn": "^4.0.0",
    "cssnano": "^3.7.5",
    "glob": "^7.0.6",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.9.3",
    "postcss": "^5.2.0",
    "postcss-cssnext": "^2.8.0",
    "rimraf": "^2.5.4",
    "sass-graph": "^2.1.2"
  },
  "devDependencies": {
    "@types/node": "^6.0.38",
    "mocha": "^3.0.2",
    "eslint": "^3.4.0",
    "tslint": "^3.15.1",
    "typescript": "^1.8.10",
    "husky": "*"
  }
}
