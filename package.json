{
  "name": "kiki-bundler",
  "version": "1.6.1",
  "description": "Batteries included frontend asset bundler",
  "main": "dist/index.js",
  "bin": {
    "kiki": "bin/kiki"
  },
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:marvinhagemeister/kiki.git"
  },
  "bugs": {
    "url": "https://github.com/marvinhagemeister/kiki/issues"
  },
  "homepage": "https://github.com/marvinhagemeister/kiki#readme",
  "scripts": {
    "test": "mocha --compilers ts:ts-node/register 'src/**/__tests__/*.spec.ts'",
    "test-watch": "npm run test -s -- -w",
    "clean": "rimraf 'dist/**/*.js' 'dist/**/*.js'",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
    "precommit": "npm run lint && npm run test",
    "prepublish": "npm run build && npm t"
  },
  "author": "Marvin Hagemeister <marvin@marvinhagemeister.de>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.5.1",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "cross-spawn": "^4.0.2",
    "glob-promise": "^1.0.6",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.10.1",
    "postcss": "^5.2.5",
    "postcss-cssnext": "^2.8.0",
    "rimraf": "^2.5.4",
    "sass-graph": "^2.1.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/chai": "^3.4.34",
    "@types/chalk": "^0.4.31",
    "@types/chokidar": "^1.4.29",
    "@types/mkdirp": "^0.3.29",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.45",
    "@types/node-sass": "0.0.29",
    "@types/rimraf": "0.0.28",
    "eslint": "^3.8.1",
    "glob-promise": "^1.0.6",
    "husky": "*",
    "mocha": "^3.1.2",
    "ts-node": "^1.6.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3"
  }
}
