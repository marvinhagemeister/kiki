{
  "name": "kiki-bundler",
  "version": "1.9.3",
  "description": "Batteries included frontend asset bundler",
  "main": "dist/index.js",
  "bin": {
    "kiki": "bin/kiki"
  },
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:marvinhagemeister/kiki.git"
  },
  "bugs": {
    "url": "https://github.com/marvinhagemeister/kiki/issues"
  },
  "homepage": "https://github.com/marvinhagemeister/kiki#readme",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "clean": "rimraf 'dist/**/*.js' 'dist/**/*.js'",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build && npm t"
  },
  "author": "Marvin Hagemeister <marvin@marvinhagemeister.de>",
  "license": "MIT",
  "dependencies": {
    "@types/glob": "^5.0.30",
    "autoprefixer": "^6.5.4",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "ci-info": "^1.1.2",
    "cross-spawn": "^5.0.1",
    "nicer-fs": "^0.2.0",
    "node-sass": "^4.0.0",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^2.9.0",
    "posthtml": "^0.10.1",
    "posthtml-parser": "^0.3.0",
    "rimraf": "^2.5.4",
    "sass-graph": "2.1.2",
    "yargs": "^6.5.0"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/chokidar": "^1.4.30",
    "@types/jest": "^21.1.8",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^8.0.57",
    "@types/node-sass": "^3.10.32",
    "@types/rimraf": "^2.0.2",
    "caniuse-db": "^1.0.30000780",
    "cross-spawn": "^5.0.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.1",
    "ts-jest": "^21.2.4",
    "ts-node": "^3.3.0",
    "tslint": "^4.0.2",
    "tslint-config-sevenval": "^0.6.4",
    "typescript": "^2.1.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --trailing-comma=all --write",
      "git add"
    ]
  }
}
